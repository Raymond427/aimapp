/* <--------------------- KEYFRAMES -------------------> */
@keyframes fadeNdrop{
	from {
		opacity: 0;
		visibility: hidden;
		transform: translateY(-10px);
	}
	to {
		opacity: 1;
		visibility: visible;
		transform: translateY(10px);
	}
}
@keyframes fadeNrise{
	from {
		opacity: 0;
		visibility: hidden;
		transform: translateY(10px);
	}
	to {
		opacity: 1;
		visibility: visible;
		transform: translateY(-10px);
	}
}
@keyframes fadeIn{
	from{
		opacity: 0;
		visibility: hidden;
	}
	to{
		opacity: 1;
		visibility: visible;
	}
}
/* <--------------------- MEDIA QUERIES -------------------> */
/* <---------------------    768px      -------------------> */
@media screen and (max-width: 768px) {
	main {
	margin-top: 59px;
	}
	.home_banner {
		height: 330px;
	}
	.home_overlay {
		padding: 50px;
	}
}
/* <---------------------    700px      -------------------> */
@media screen and (max-width: 700px)
{
  .news figure {
    margin-top: -10px;
    width: 100%;
  }
  .news figure img, iframe {
    height: 300px;
		margin-top: 10px;
    width: 100%;
    float: none;
  }

  .news figcaption {
    float: none;
  }

  .news { /*All News blocks*/
    width: 80%;
  }
}
/* <---------------------    505px      -------------------> */
@media screen and (max-width: 505px)
{
  .board_member { /*All Exec blocks*/
    width: 60%;
  }

  .board_member figure img {
    float: none;
    width: 100%;
    height: 100%;
  }
  .board_member figure figcaption {
    float: none;
    width: auto;
    padding-bottom: 5%;
  }

  .board_member figure figcaption p {
    font-size: 16px;
  }

  .home_mid img {
    width: 290px;
  }
}
/* <---------------------    480px      -------------------> */
@media screen and (max-width: 480px)
{
	.home_overlay {
		padding: 5px;
	}
}
